{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends":  [
        "airbnb-base",
        "plugin:json/recommended",
        "plugin:prettier/recommended"
      ],
    "overrides": [
        {
          "files": ["*.md"],
          "parser": "eslint-plugin-markdownlint/parser",
          "extends": ["plugin:markdownlint/recommended"],
          "rules": {
            "markdownlint/md013": "off",
            "markdownlint/md033": "off"
          }
        }
      ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    }
}
